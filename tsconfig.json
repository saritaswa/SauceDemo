{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "tests/**/*.ts",
    "pages/**/*.ts",
    "base/**/*.ts",
    "Utilities/**/*.ts"
, "tests/.spec.ts"  ]
}